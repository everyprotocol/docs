{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Every Protocol",
  "logo": {
    "light": "/logo/docs-black.svg",
    "dark": "/logo/docs-white.svg"
  },
  "colors": {
    "primary": "#00D0FF",
    "light": "#00DDFF",
    "dark": "#00D0FF"
  },
  "favicon": "/every-favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Get Started",
        "global": {
          "anchors": [
            {
              "icon": "code",
              "anchor": "Developer Portal",
              "href": "https://dev.every.fun"
            },
            {
              "icon": "faucet-drip",
              "anchor": "Testnet Faucet",
              "href": "https://faucet.every.fun"
            },
            {
              "icon": "signal",
              "anchor": "Status Page",
              "href": "https://status.every.fun/status/every"
            }
          ]
        },
        "groups": [
          {
            "group": "Introduction",
            "pages": ["get-started/every"]
          },
          {
            "group": "Quickstart",
            "pages": [
              "get-started/wallets",
              "get-started/register-matters",
              "get-started/register-sets",
              "get-started/register-kinds"
            ]
          }
        ]
      },
      {
        "tab": "Learn",
        "groups": [
          {
            "group": "Concepts",
            "pages": [
              "learn/concepts/objects",
              "learn/concepts/elements",
              "learn/concepts/kinds",
              "learn/concepts/sets",
              "learn/concepts/relations"
            ]
          },
          {
            "group": "Advanced",
            "pages": ["learn/concepts/pocc", "learn/concepts/projection-layer", "learn/concepts/ovm"]
          }
        ]
      },

      {
        "tab": "Cookbook",
        "groups": [
          {
            "group": "Guides",
            "pages": [
              "cookbook/get-started",
              "cookbook/register-matters",
              "cookbook/register-values",
              "cookbook/register-uniques",
              "cookbook/register-kinds",
              "cookbook/register-sets",
              "cookbook/register-relations",
              "cookbook/relate-objects",
              "cookbook/mint-objects"
            ]
          },
          {
            "group": "Developers",
            "pages": ["cookbook/writing-kind-contracts", "cookbook/writing-set-contracts"]
          }
        ]
      },
      {
        "tab": "Contracts",
        "groups": [
          {
            "group": "Core",
            "pages": [
              "contracts/core/SetRegistry",
              "contracts/core/OmniRegistry",
              "contracts/core/KindRegistry",
              "contracts/core/ElementRegistry",
              "contracts/core/ObjectMinter"
            ]
          },
          {
            "group": "Periphery",
            "pages": [
              "contracts/periphery/ISet",
              "contracts/periphery/IInteroperable",
              "contracts/periphery/IRemoteMintable",
              "contracts/periphery/ISetRegistryAdmin",
              "contracts/periphery/IObjectMinterAdmin"
            ]
          },
          { "group": "Deployment", "pages": ["contracts/deployment"] }
        ]
      },
      {
        "tab": "Network",
        "groups": [
          {
            "group": "Network",
            "pages": [
              "network/overview",
              "network/accounts",
              "network/transactions",
              "network/state",
              "network/contracts",
              "network/ovm",
              "network/tokenomics",
              "network/nodes",
              "network/deployment"
            ]
          }
        ]
      },
      {
        "tab": "Reference",
        "groups": [
          {
            "group": "CLI",
            "pages": ["reference/every-cli", "reference/kasc"]
          },
          {
            "group": "API",
            "pages": ["reference/kind-as", "reference/core", "reference/periphery"]
          },
          {
            "group": "Resources",
            "pages": ["reference/resources/foundry", "reference/resources/assemblyscript", "reference/resources/bun"]
          }
        ]
      }
    ]
  },
  "navbar": {
    "links": [
      {
        "label": "Github",
        "href": "https://github.com/everyprotocol"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Every.fun",
      "href": "https://efun.vercel.app"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/everyprotocol",
      "github": "https://github.com/everyprotocol",
      "website": "https://efun.vercel.app"
    }
  }
}
